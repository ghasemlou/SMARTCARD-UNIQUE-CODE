
SELECT *
FROM (
SELECT 
DISTINCT(ALIAS_NO) AS ID02,
COUNT(ALIAS_NO) AS TRIP_PER_DAY02,
"2" AS TRIPDATE02,
 FROM `smart-card-data.CARDDATA.DATE002` 
 GROUP BY ALIAS_NO) AS DATE2
 
 FULL OUTER JOIN
 (SELECT *
FROM (
SELECT 
DISTINCT(ALIAS_NO) AS ID01,
COUNT(ALIAS_NO) AS TRIP_PER_DAY01,
"1" AS TRIPDATE01,
 FROM `smart-card-data.CARDDATA.DATE001` 
 GROUP BY ALIAS_NO))AS DATE1
 ON
 DATE2.ID02 = DATE1.ID01

 
