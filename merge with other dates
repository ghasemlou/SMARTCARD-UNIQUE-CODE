SELECT 
DISTINCT(ALIAS_NO) AS ID,
COUNT(ALIAS_NO) AS TRIP_PER_DAY,
"2" AS TRIPDATE,
 FROM `smart-card-data.CARDDATA.DATE002` AS DATE002
 GROUP BY ALIAS_NO
UNION ALL
SELECT 
DISTINCT(ALIAS_NO) AS ID01,
COUNT(ALIAS_NO) AS TRIP01,
"1" AS TRIPDATE,
 FROM `smart-card-data.CARDDATA.DATE001` AS DATE001
 GROUP BY ALIAS_NO,
